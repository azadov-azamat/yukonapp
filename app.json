{
  "expo": {
    "name": "Yukon",
    "slug": "yukonapp",
    "version": "2.1.4",
    "orientation": "portrait",
    "icon": "./assets/images/icon.jpg",
    "scheme": "yukon.uz.app",
    "userInterfaceStyle": "automatic",
		"newArchEnabled": true,
    "runtimeVersion": "2.1.2",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "yukon.uz.app",
      "buildNumber": "1.1.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
				"NSLocationWhenInUseUsageDescription": "This app uses your location to show relevant info nearby."
      }
    },
    "android": {
      "package": "yukon.uz.app",
      "versionCode": 8,
      "permissions": [],
			"edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.jpg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "splash": {
      "image": "./assets/images/splash-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 36,
            "targetSdkVersion": 36,
            "buildToolsVersion": "36.0.0",
            "minSdkVersion": 24
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "355249bb-7f55-452a-80b0-81a49689458f"
      }
    },
    "owner": "azamat_azadov"
  }
}
